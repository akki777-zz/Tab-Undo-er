{
	"manifest_version": 2,
	"name": "Tab Undo-er",
	"description": "Undo Closed Tabs",
	"version": "0.0.1",
	"browser_action": {
		"default_icon": "icon_48.png"
	},
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png"
	},
	"permissions": [
		"tabs",
		"storage"
	],
	"background": {
		"scripts": [
			"background.js"
		],
		"persistent": false
	},
	"commands": {
		"undo-tab": {
			"suggested_key": {
				"default": "Ctrl+Shift+Z"
			},
			"description": "Tab Undo Shortcut"
		}
	}
}